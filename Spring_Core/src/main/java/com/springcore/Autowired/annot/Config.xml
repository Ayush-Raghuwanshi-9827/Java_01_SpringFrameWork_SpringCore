<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

      <context:annotation-config/>
      
	  <bean name="address1" class="com.springcore.Autowired.annot.Address">
		<property name="city" value="indoor"/> 
		<property name="state" value="M.P"/>
	  </bean>
	  
      <bean name="address2" class="com.springcore.Autowired.annot.Address">
		<property name="city" value="Bhopal"/> 
		<property name="state" value="M.P"/>
	  </bean>
	  
	  <bean name="address3" class="com.springcore.Autowired.annot.Address">
		<property name="city" value="Sagar"/> 
		<property name="state" value="M.P"/>
	  </bean>
	  
	  <bean name="st" class="com.springcore.Autowired.annot.Student"></bean>
</beans>